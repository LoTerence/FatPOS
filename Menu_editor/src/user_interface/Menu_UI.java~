    private void removeItemButtonActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_removeItemButtonActionPerformed
     DefaultListModel model = (DefaultListModel) orderJList.getModel();
    if(model == null)
    {
        model = new DefaultListModel();
        orderJList.setModel(model);
    }
    model.removeElement(orderJList.getSelectedValue()); 
    
    //order.removeFromTotal(5);
    }//GEN-LAST:event_removeItemButtonActionPerformed

    private void category1Button1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_category1Button1ActionPerformed
     DefaultListModel model = (DefaultListModel) orderJList.getModel();
    if(model == null)
    {
        model = new DefaultListModel();
        orderJList.setModel(model);
    }
    model.addElement(category1Button1.getText());
   // lm1.removeElement(listaTitolari.getSelectedValue());   
    }//GEN-LAST:event_category1Button1ActionPerformed

